import java.util.*;
class Demo{
  public static void main (String args[]){
 System.out.println("name: Y.satyanarayana\nsap id: 51834602");
 Scanner sc=new Scanner(System.in);
 System.out.println("input ");
 int n=sc.nextInt();
 int c=0;
 while(n>0){
 int rem=n%10;
 if(rem%2==0){
      c++;
      }
      n=n/10;
    }
    System.out.println("output "+c);
  }
}
